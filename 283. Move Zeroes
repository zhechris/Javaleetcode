class Solution {
    public void moveZeroes(int[] nums) {
        int size = nums.length;  
        int i = 0,pos = 0;  
        for(i=0;i<size;++i){  
            if(nums[i]!=0){  
                nums[pos]=nums[i];  
                pos++;  
            }  
        }  
        for(i=pos;i<size;++i){  
            nums[i] = 0;  
        }
    }
}
