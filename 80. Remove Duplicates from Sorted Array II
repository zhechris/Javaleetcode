class Solution {
    public int removeDuplicates(int[] nums) {
        List<Integer> list = new ArrayList<Integer>();
        for (int i=0; i<nums.length; i++) {
            if(!list.contains(nums[i])) {
                list.add(nums[i]);
            }
        }
        for(int i=0;i<list.size();i++)//这道题有个坑就是只能截取原来数组的前length个，所以要赋值过去
        {
            nums[i]=list.get(i);
        }
        return list.size();
    }
}
